{
  "name": "contact",
  "base": "PersistedModel",
  "strict": false,
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "fax": {
      "type": "string"
    },
    "email": {
      "type": "string",
      "id": false,
      "required": true
    },
    "firstName": {
      "type": "string",
      "id": false,
      "required": true
    },
    "lastName": {
      "type": "string",
      "id": false,
      "required": true
    },
    "mobile": {
      "type": "string",
      "required": true
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "salutation": {
      "type": "string",
      "required": true
    },
    "title": {
      "type": "string",
      "required": false
    },
    "investorId": {
      "type": "number",
      "required": true,
      "index": true,
      "comments": "FK to investors"
    }
  },
  "validations": [],
  "relations": {
    "investor": {
      "type": "belongsTo",
      "model": "investor",
      "foreignKey": "investorId"
    }
  },
  "acls": [],
  "methods": {}
}
